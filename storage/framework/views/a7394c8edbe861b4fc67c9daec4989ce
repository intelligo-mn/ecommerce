                </div>


                <?php if(count($lastItems) > 0): ?>
                    <div class="jscroll" data-auto="<?php echo getcong('AutoLoadLists') ?: 'false'; ?>">
                    <?php echo $__env->make('pages.catpostloadpage', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>
                    </div>
                    <?php else: ?>
                    <?php echo $__env->make('errors.emptycontent', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>

                <?php endif; ?>

            </div>
            <div class="sidebar">

                <?php foreach(\App\Widgets::where('type', 'CatSide')->where('display', 'on')->get() as $widget): ?>
                    <?php echo $widget->text; ?>

                <?php endforeach; ?>

                <?php echo $__env->make("_widgets/facebooklike", array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>

            </div>
        </div>

    </div>


<?php $__env->stopSection(); ?>
<?php echo $__env->make("app", array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>